{
  "name": "test-canvas-worker-wasm",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "v": "node -v",
    "type-check": "tsc --noemit",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettify": "prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "prettify-watch": "onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}",
    "onchange:wasm": "onchange -v -i 'src/engine/wasm/makefile' 'src/engine/wasm/asconfig.json' 'src/engine/wasm/src/**/*.ts' -- make --no-print-directory -C src/engine/wasm ",
    "onchange:images": "onchange -v -i 'src/assets/makefile' 'src/assets/images/preprocImagesList.mjs' 'src/assets/images/images.res' -- make --no-print-directory -C src/assets images",
    "onchange:strings": "onchange -v -i 'src/assets/makefile' 'src/assets/strings/preprocSringsList.mjs' 'src/assets/strings/strings.res' -- make --no-print-directory -C src/assets strings",
    "onchange:assets": "run-p --race onchange:images onchange:strings",
    "watch": "webpack --watch",
    "build:assets": "cd src/assets && make all",
    "build:asc": "cd src/engine/wasm && make build_asc",
    "build:wat": "cd src/engine/wasm && make build_wat",
    "build:wasm": "npm run build:asc && npm run build:wat",
    "webpack": "webpack --config ./webpack.config.ts",
    "build": "npm run build:assets && npm run build:wasm && webpack",
    "server": "webpack-dev-server",
    "launch": "npm run cleandist && npm run build && webpack-dev-server",
    "start": "run-p --race launch onchange:wasm onchange:assets",
    "prod": "webpack --production",
    "clean": "rimraf dist/ && cd src/engine/wasm && make clean",
    "cleandist": "rimraf dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@assemblyscript/loader": "^0.21.6",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@types/assert": "^1.5.6",
    "@types/dat.gui": "^0.7.7",
    "@types/node": "^18.11.0",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.21",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "as-big": "^0.2.0",
    "as-bignum": "^0.2.23",
    "assemblyscript": "^0.21.6",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.5",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "os-browserify": "^0.3.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-essentials": "^9.3.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "wabt": "^1.0.30",
    "wasm-loader": "^1.3.0",
    "wasm-opt": "^1.3.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "dat.gui": "^0.7.9",
    "fast-glob": "^3.2.12",
    "file-type": "^18.0.0",
    "pako": "^2.0.4",
    "preact": "^10.11.2",
    "preact-fullscreen": "^0.1.2",
    "react": "^18.2.0",
    "screenfull": "^6.0.2"
  }
}
